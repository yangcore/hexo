<%   
    var _bootstrap = '//cdn.bootcss.com/bootstrap/3.3.4/css/bootstrap.min.css?rev=3.3.4';
    var _fontAwe =  theme.fontAwesome || '/css/thirdParty/font-awesome.min.css?rev=4.7.0';
    var _gitmentStyle = '//imsun.github.io/gitment/style/default.css';
    var _highlightStyle = '../../../css/default.min.css';
    var style = '/css/style.css?rev=@@hash';
%>

<% if(theme.gitment.enable && is_post()){ %>
    <%- css([_gitmentStyle]) %>
<% } %>

<% if(theme.highlightTheme && is_post()){ %>
    <%- css([_highlightStyle]) %>
<% } %>

<%- css([_bootstrap, _fontAwe, style]) %>



